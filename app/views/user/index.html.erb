<table>
  <thead>
    <tr>
      <th>id</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Username</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td class="user-id"><%= user.id %></td>
        <td class="user-first-name"><%= user.first_name %></td>
        <td class="user-last-name"><%= user.last_name %></td>
        <td class="user-username"><%= user.username %></td>
        <td class="user-email"><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>